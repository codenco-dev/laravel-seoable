{
    "name": "mad-web/laravel-seoable",
    "description": "Easy to map your eloquent fields to seo properties",
    "type": "library",
    "keywords": [
        "mad-web",
        "laravel-seoable",
        "seo",
        "seotools",
        "opengraph",
        "twitter-card",
        "webmasters",
        "metatags"
    ],
    "homepage": "https://github.com/mad-web/laravel-seoable",
    "license": "MIT",
    "authors": [
        {
            "name": "Mad Web",
            "email": "madweb.dev@gmail.com",
            "homepage": "https://github.com/mad-web",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.4",
        "illuminate/database": "^5.4",
        "artesaos/seotools": "^0.10.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "MadWeb\\Seoable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadWeb\\Seoable\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadWeb\\Seoable\\SeoableServiceProvider"
            ]
        }
    }
}
